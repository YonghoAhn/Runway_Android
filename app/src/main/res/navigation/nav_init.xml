<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_init"
            app:startDestination="@id/loginFragment">

    <fragment android:id="@+id/loginFragment" android:name="moe.misakachan.runway.Fragments.LoginFragment"
              android:label="login_fragment" tools:layout="@layout/login_fragment">
        <action android:id="@+id/action_loginFragment_to_registerFragment" app:destination="@id/registerFragment"/>
    </fragment>
    <fragment android:id="@+id/registerFragment" android:name="moe.misakachan.runway.Fragments.RegisterFragment"
              android:label="register_fragment" tools:layout="@layout/register_fragment">
        <action android:id="@+id/action_registerFragment_to_wiFiFragment" app:destination="@id/wiFiFragment"/>
    </fragment>
    <fragment android:id="@+id/wiFiFragment" android:name="moe.misakachan.runway.Fragments.WiFiFragment"
              android:label="wi_fi_fragment" tools:layout="@layout/wi_fi_fragment"/>
</navigation>